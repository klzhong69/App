        android:layout_marginEnd="@dimen/dp_20"
        android:layout_toStartOf="@+id/imageView111"
        app:srcCompat="@drawable/dk_network_bar" />

    <ImageView
        android:id="@+id/imageView113"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_1"
        android:layout_alignStart="@+id/imageView19"
        android:layout_alignTop="@+id/imageView19"
        android:layout_alignEnd="@+id/imageView19"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_120"
        android:layout_marginEnd="@dimen/dp_20"

        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/imageView114"
        android:layout_width="@dimen/dp_30"
        android:layout_height="@dimen/dp_30"
        android:layout_below="@+id/textView114"
        android:layout_alignStart="@+id/textView114"
        android:layout_marginTop="@dimen/dp_10"
        app:srcCompat="@android:drawable/btn_star_big_on" />

    <TextView
        android:id="@+id/textView132"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView114"
        android:layout_alignBottom="@+id/imageView114"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_toEndOf="@+id/imageView114"
        android:gravity="center_vertical"
        android:text="今日推荐"
        android:textColor="#aaffffff"
        android:textSize="@dimen/sp_16" />

    <ImageView
        android:id="@+id/imageView115"
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40"
        android:layout_below="@+id/imageView113"
        android:layout_alignStart="@+id/imageView19"
        android:layout_marginStart="@dimen/dp_70"
        android:layout_marginTop="@dimen/dp_20"
        app:srcCompat="@drawable/dk_float_button" />

    <ImageView
        android:id="@+id/imageView116"
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40"
        android:layout_alignTop="@+id/imageView115"
        android:layout_marginStart="@dimen/dp_60"
        android:layout_toEndOf="@+id/imageView115"
        app:srcCompat="@drawable/dk_float_button" />

    <ImageView
        android:id="@+id/imageView117"
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40"
        android:layout_alignTop="@+id/imageView116"
        android:layout_marginStart="@dimen/dp_60"
        android:layout_toEndOf="@+id/imageView116"
        app:srcCompat="@drawable/dk_float_button" />

    <ImageView
        android:id="@+id/imageView118"
        android:layout_width="@dimen/dp_40"
        android:layout_height="@dimen/dp_40"
        android:layout_alignTop="@+id/imageView117"
        android:layout_marginStart="@dimen/dp_60"
        android:layout_toEndOf="@+id/imageView117"
        app:srcCompat="@drawable/dk_float_button" />

</RelativeLayout>
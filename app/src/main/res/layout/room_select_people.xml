<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_102"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_100"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:background="@color/qmui_config_color_white">

        <com.qmuiteam.qmui.widget.QMUIRadiusImageView
            android:id="@+id/imageView18"
            android:layout_width="@dimen/dp_70"
            android:layout_height="@dimen/dp_70"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_15"
            android:src="@drawable/back2"
            app:qmui_border_width="0dp"
            app:qmui_corner_radius="@dimen/dp_50" />

        <TextView
            android:id="@+id/textView66"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageView18"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_toEndOf="@+id/imageView18"
            android:text="胡楠健身"
            android:textSize="@dimen/sp_20" />

        <ImageView
            android:id="@+id/imageView66"
            android:layout_width="@dimen/dp_40"
            android:layout_height="@dimen/dp_20"
            android:layout_alignTop="@+id/textView66"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_toEndOf="@+id/textView66"
            app:srcCompat="@drawable/l3" />


        <TextView
            android:id="@+id/textView114"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView66"
            android:layout_alignStart="@+id/textView66"
            android:layout_marginTop="@dimen/dp_5"
            android:gravity="center_vertical"
            android:text="ID2634589"
            android:textSize="@dimen/sp_16" />

        <TextView
            android:id="@+id/textView115"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="@dimen/dp_35"
            android:layout_marginEnd="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_35"
            android:gravity="center"
            android:paddingStart="@dimen/dp_10"
            android:paddingTop="@dimen/dp_2"
            android:paddingEnd="@dimen/dp_10"
            android:paddingBottom="@dimen/dp_2"
            android:text="删除"
            android:textColor="@color/background_error"
            android:textSize="@dimen/sp_20" />

    </RelativeLayout>


</RelativeLayout>
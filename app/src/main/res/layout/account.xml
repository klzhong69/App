<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.qmuiteam.qmui.widget.QMUIRadiusImageView
        android:id="@+id/imageView41"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="0dp"
        android:src="@color/qmui_config_color_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include"
        app:qmui_border_width="0dp"
        app:qmui_corner_radius="@dimen/dp_15" />

    <ImageView
        android:id="@+id/imageView76"
        android:layout_width="@dimen/dp_30"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView41"
        android:layout_alignTop="@+id/imageView41"
        android:layout_alignBottom="@+id/imageView41"
        android:layout_marginStart="@dimen/dp_20"
        app:layout_constraintBottom_toBottomOf="@+id/imageView41"
        app:layout_constraintStart_toStartOf="@+id/imageView41"
        app:layout_constraintTop_toTopOf="@+id/imageView41"
        app:srcCompat="@drawable/weixin_h" />


    <TextView
        android:id="@+id/textView87"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView76"
        android:layout_alignBottom="@+id/imageView76"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_toEndOf="@+id/imageView76"
        android:gravity="center"
        android:text="微信"
        app:layout_constraintBottom_toBottomOf="@+id/imageView41"
        app:layout_constraintStart_toEndOf="@+id/imageView76"
        app:layout_constraintTop_toTopOf="@+id/imageView41" />

    <TextView
        android:id="@+id/textView169"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/imageView41"
        android:layout_alignBottom="@+id/imageView41"
        android:layout_marginStart="@dimen/dp_10"
        android:gravity="center"
        android:layout_toEndOf="@+id/textView87"
        android:text="[dali-chen]" />

    <TextView
        android:id="@+id/textView170"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/imageView41"
        android:layout_alignBottom="@+id/imageView41"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_toEndOf="@+id/textView169"
        android:gravity="center"
        android:text="默认"
        android:textColor="#D81818" />

    <TextView
        android:id="@+id/textView171"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView41"
        android:layout_alignBottom="@+id/imageView41"
        android:layout_alignParentEnd="true"
        android:gravity="center"
        android:layout_marginEnd="@dimen/dp_10"
        android:text="取消" />


</RelativeLayout>
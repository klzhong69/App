<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_145"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_140"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:background="@color/qmui_config_color_white">

        <com.qmuiteam.qmui.widget.QMUIRadiusImageView
            android:id="@+id/imageView18"
            android:layout_width="@dimen/dp_110"
            android:layout_height="@dimen/dp_110"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_10"
            android:src="@drawable/back2"
            app:qmui_border_width="0dp"
            app:qmui_corner_radius="@dimen/dp_10" />

        <TextView
            android:id="@+id/textView66"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageView18"
            android:layout_marginStart="@dimen/dp_30"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_toEndOf="@+id/imageView18"
            android:text="Phakamile Sikali"
            android:textSize="@dimen/sp_24" />

        <ImageView
            android:id="@+id/imageView66"
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignTop="@+id/textView113"
            android:layout_alignBottom="@+id/textView113"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_toStartOf="@+id/textView113"
            app:srcCompat="@drawable/henter" />

        <TextView
            android:id="@+id/textView113"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView18"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="4dp"
            android:gravity="center_vertical"
            android:text="2345"
            android:textColor="#9d9d9d" />

        <TextView
            android:id="@+id/textView114"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView66"
            android:layout_alignStart="@+id/textView66"
            android:layout_marginTop="@dimen/dp_5"
            android:gravity="center_vertical"
            android:text="点歌一如既往"
            android:textColor="#6d6d6d"
            android:textSize="@dimen/sp_18"/>

        <TextView
            android:id="@+id/textView117"
            android:layout_width="@dimen/dp_50"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView114"
            android:layout_alignBottom="@+id/imageView18"
            android:layout_marginBottom="@dimen/dp_5"
            android:background="@drawable/label"
            android:padding="@dimen/dp_5"
            android:text="热门"
            android:textColor="#ffffff"
            android:gravity="center"
            android:textSize="@dimen/sp_16" />

        <TextView
            android:id="@+id/textView118"
            android:layout_width="@dimen/dp_50"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView117"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_toEndOf="@+id/textView117"
            android:background="@drawable/label"
            android:gravity="center"
            android:padding="@dimen/dp_5"
            android:text="CV"
            android:textColor="#ffffff"
            android:textSize="@dimen/sp_16" />

        <TextView
            android:id="@+id/textView119"
            android:layout_width="@dimen/dp_50"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView118"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_toEndOf="@+id/textView118"
            android:background="@drawable/label"
            android:gravity="center"
            android:padding="@dimen/dp_5"
            android:text="德国"
            android:textColor="#ffffff"
            android:textSize="@dimen/sp_16" />

        <ImageView
            android:id="@+id/imageView95"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_alignTop="@+id/imageView18"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_toEndOf="@+id/textView66"
            app:srcCompat="@drawable/dk_crash_catch" />

    </RelativeLayout>

</RelativeLayout>
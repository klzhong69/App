<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_105"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_100"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:background="@color/qmui_config_color_white">

        <TextView
            android:id="@+id/textView68"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_35"
            android:layout_marginBottom="@dimen/dp_35"
            android:gravity="center_vertical"
            android:text="默认播单"
            android:textSize="@dimen/sp_20" />

        <TextView
            android:id="@+id/textView69"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView68"
            android:layout_alignBottom="@+id/textView68"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_toEndOf="@+id/textView68"
            android:gravity="center_vertical"
            android:text="[26]"
            android:textSize="@dimen/sp_16" />


        <ImageView
            android:id="@+id/imageView67"
            android:layout_width="@dimen/dp_30"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView69"
            android:layout_alignBottom="@+id/textView69"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="@dimen/dp_20"
            app:srcCompat="@drawable/qmui_icon_chevron" />
    </RelativeLayout>



</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/component10">

    <RelativeLayout
        android:id="@+id/recyclerbutc10"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_400"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/recyclerc10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/recyclerbutc10"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
       >

        <com.qmuiteam.qmui.widget.QMUIRadiusImageView
            android:id="@+id/imageViewc10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textViewc10"
            android:layout_alignTop="@+id/imageViewc10s"
            android:layout_alignEnd="@+id/textViewc10"
            android:layout_alignBottom="@+id/butc10"
            android:layout_marginBottom="@dimen/dp_m_20"
            android:src="@color/qmui_config_color_white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:qmui_border_width="0dp"
            app:qmui_corner_radius="15dp" />


        <ImageView
            android:id="@+id/imageViewc10s"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:layout_alignStart="@+id/textViewc10"
            android:layout_alignEnd="@+id/textViewc10"
            android:layout_alignParentTop="true"
            android:layout_marginTop="@dimen/dp_30"
            app:srcCompat="@drawable/ima" />

        <TextView
            android:id="@+id/textViewc10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView148"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="@dimen/dp_100"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_100"
            android:gravity="center"
            android:padding="@dimen/dp_20"
            android:text="欢迎来到6354589" />

        <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
            android:id="@+id/butc10"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_50"
            android:layout_below="@+id/textViewc10"
            android:layout_alignStart="@+id/textViewc10"
            android:layout_alignEnd="@+id/textViewc10"
            android:layout_marginStart="@dimen/dp_100"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_100"
            android:layout_marginBottom="@dimen/dp_20"
            android:gravity="center"
            android:padding="@dimen/dp_10"
            android:text="确定"
            android:textColor="@color/qmui_config_color_white"
            android:textSize="@dimen/sp_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:qmui_backgroundColor="@color/tabbarcolor"
            app:qmui_borderWidth="0px"
            app:qmui_radius="@dimen/dp_30" />

        <TextView
            android:id="@+id/textView148"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/imageViewc10s"
            android:layout_alignTop="@+id/imageViewc10s"
            android:layout_alignEnd="@+id/imageViewc10s"
            android:layout_alignBottom="@+id/imageViewc10s"
            android:gravity="center"
            android:padding="@dimen/dp_10"
            android:text="房间公告"
            android:textColor="#ffffff"
            android:textSize="@dimen/sp_20" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/recyclerbutc10s"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/recyclerc10"
        android:layout_alignParentStart="true">

    </RelativeLayout>
</RelativeLayout>